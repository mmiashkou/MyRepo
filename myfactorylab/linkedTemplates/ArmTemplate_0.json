				"type": "Managed",
				"typeProperties": {
					"computeProperties": {
						"location": "North Europe",
						"nodeSize": "Standard_D2_v3",
						"numberOfNodes": 1,
						"maxParallelExecutionsPerNode": 2
					},
					"ssisProperties": {
